package data;

import org.newdawn.slick.opengl.*;

public class Enemy {
	private int x, y, width, height, health;
	private float speed;
	Texture texture;
	
	public Enemy(Texture texture, int x, int y, int width, int height, float speed){
		this.texture = texture;
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
		this.speed = speed;
	}
	
	public void Draw;
}
